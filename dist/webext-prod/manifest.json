{
  "manifest_version": 2,
  "name": "Wiktionary++",
  "version": "0.1.1",
  "description": "Wiktionary extension",
  "content_scripts": [
    {
      "css": [
        "/styles.css"
      ],
      "js": [
        "/main.js"
      ],
      "matches": [
        "https://pl.wiktionary.org/wiki/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "/background/eventListener.js"
    ],
    "persistent": false
  },
  "permissions": [
    "notifications"
  ],
  "icons": {
    "128": "/images/logo.png"
  }
}